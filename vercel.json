{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "github": {
    "silent": true
  },
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "public": true,
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "domains": ["localhost"],
    "minimumCacheTTL": 60,
    "formats": ["image/webp"]
  }
}
